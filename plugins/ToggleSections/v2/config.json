{
    "info": {
        "name": "Toggle Sections",
        "authors": ["kettui/ciN"],
        "version": "2.0~Alpha",
        "description": "A plugin that allows you to toggle the visibility of certain sections on the fly",
        "repository": "https://github.com/kettui/BetterDiscord-extensions.git",
        "homepage": "https://github.com/kettui/BetterDiscord-extensions",
        "reloadable": true
    },
    "defaultSettings": [{
            "id": "color",
            "type": "colorpicker",
            "value": "#738BD7",
            "description": "Color of the toggle buttons"
        },
        {
            "id": "guilds",
            "type": "object",
            "description": "Guild list",
            "value": {
                "type": "container",
                "label": "Guild list",
                "className": "guilds-wrapper",
                "position": "right",
                "isEnabled": true,
                "isClosed": false,
                "hotkey": 24
            }
        }, {
            "id": "channels",
            "type": "object",
            "description": "Channel list",
            "value": {
                "type": "container",
                "label": "Channel list",
                "className": "channels-wrap",
                "position": "right",
                "isEnabled": true,
                "isClosed": false,
                "hotkey": 3
            }
        }
    ],
    "permissions": []
}
